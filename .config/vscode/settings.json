{
  "editor.fontFamily": "Dank Mono",
  "editor.fontSize": 14,
  "editor.tabSize": 2,
  "editor.fontLigatures": true,
  "editor.cursorBlinking": "smooth",
  "editor.renderLineHighlight": "none",
  "editor.minimap.enabled": false,
  "editor.dragAndDrop": false,
  "editor.smoothScrolling": true,
  "editor.formatOnSave": true,
  "editor.folding": false,
  "editor.wordWrap": "on",
  "editor.lineNumbers": "off",
  "editor.guides.indentation": false,
  "editor.glyphMargin": false,
  "editor.overviewRulerBorder": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.columnSelection": false,
  "editor.suggestSelection": "first",
  "editor.suggest.preview": true,
  "editor.bracketPairColorization.enabled": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "breadcrumbs.enabled": false,
  "search.mode": "newEditor",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorBlinking": true,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "workbench.activityBar.visible": false,
  "workbench.settings.editor": "json",
  "workbench.colorTheme": "Gatito Theme",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.editor.showIcons": false,
  "workbench.iconTheme": null,
  "workbench.editor.tabCloseButton": "off",
  "workbench.enableExperiments": false,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.statusBar.visible": false,
  "workbench.editor.wrapTabs": true,
  "workbench.layoutControl.enabled": false,
  "window.title": "${activeEditorMedium} ${dirty}",
  "files.trimTrailingWhitespace": true,
  "files.defaultLanguage": "markdown",
  "telemetry.telemetryLevel": "error",
  "go.lintTool": "golangci-lint",
  "go.lintFlags": ["--fast"],
  "quokka.showOutputOnStart": false,
  "[markdown]": {
    "editor.fontFamily": "Helvetica"
  },
  "[javascript][typescript][typescriptreact][html][json][jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
